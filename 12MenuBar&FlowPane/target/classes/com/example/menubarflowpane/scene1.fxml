<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.input.KeyCodeCombination?>
<?import javafx.scene.layout.FlowPane?>
<FlowPane prefHeight="430.0" prefWidth="895.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="com.example.menubarflowpane.HelloController">
    <children>
        <MenuBar fx:id="menuID" prefHeight="25.0" prefWidth="965.0">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" text="New"/>
                        <MenuItem mnemonicParsing="false" text="Open"/>
                        <MenuItem mnemonicParsing="false" onAction="#saved" text="Save">
                            <accelerator>
                                <KeyCodeCombination alt="UP" code="S" control="DOWN" meta="UP" shift="UP"
                                                    shortcut="UP"/>
                            </accelerator>
                        </MenuItem>
                        <CheckMenuItem mnemonicParsing="false" text="AutoSave"/>
                        <SeparatorMenuItem mnemonicParsing="false"/>
                        <MenuItem mnemonicParsing="false" text="Open Recent"/>
                        <MenuItem mnemonicParsing="false" text="Close Project"/>
                        <MenuItem mnemonicParsing="false" text="Options"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Undo"/>
                        <MenuItem mnemonicParsing="false" text="Redo"/>
                        <SeparatorMenuItem mnemonicParsing="false"/>
                        <MenuItem mnemonicParsing="false" text="Cut"/>
                        <MenuItem mnemonicParsing="false" text="Copy"/>
                        <MenuItem mnemonicParsing="false" text="Paste"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="View">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Tools Window"/>
                        <MenuItem mnemonicParsing="false" text="Appearance"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Run">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Build Program"/>
                        <MenuItem mnemonicParsing="false" text="Run Program"/>
                        <MenuItem mnemonicParsing="false" text="Debug Program"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="? Help"/>
                        <MenuItem mnemonicParsing="false" text="Check for updates"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <Button mnemonicParsing="false" text="Button"/>
        <Button mnemonicParsing="false" text="Button"/>
        <Button mnemonicParsing="false" text="Button"/>
        <Button mnemonicParsing="false" text="Button"/>
        <Button mnemonicParsing="false" text="Button"/>
        <Button mnemonicParsing="false" text="Button"/>
        <Button mnemonicParsing="false" text="Button"/>
        <Button mnemonicParsing="false" text="Button"/>
        <Button mnemonicParsing="false" text="Button"/>
        <Button mnemonicParsing="false" text="Button"/>
        <Button mnemonicParsing="false" text="Button"/>
        <Button mnemonicParsing="false" text="Button"/>
        <Button mnemonicParsing="false" text="Button"/>
        <Button mnemonicParsing="false" text="Button"/>
        <Button mnemonicParsing="false" text="Button"/>
        <Button mnemonicParsing="false" text="Button"/>
        <Button mnemonicParsing="false" text="Button"/>
        <Button mnemonicParsing="false" text="Button"/>
        <Button mnemonicParsing="false" text="Button"/>
        <Button mnemonicParsing="false" text="Button"/>
        <Button mnemonicParsing="false" text="Button"/>
    </children>
</FlowPane>
